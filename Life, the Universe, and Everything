#define _2 cin.tie(NULL)
#define _3 cout.tie(NULL)

using namespace std;
int main()
{
    //_1;_2;_3;
    int n;
    while(cin >> n)
    {
        if(n != 42)
        cout << n << "\n";
        else
            break;
    }
    return 0;
}
